<UserControl
    x:Class="PrivateWiki.UI.Controls.DiffControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid>
        <StackPanel>
            <WebView
                    x:Name="DiffWebView"
                    NavigationStarting="DiffWebView_OnNavigationStarting"
                    NavigationCompleted="DiffWebView_OnNavigationCompleted"
                    MinWidth="800"
                    MinHeight="500"
                    Height="Auto"/>
        </StackPanel>
    </Grid>
</UserControl>
